{
    "name": "do-while/contao-timetracker-bundle",
    "description": "Simple Zeiterfassung",
    "keywords": ["contao", "contentelement", "timetracker"],
    "type": "contao-bundle",
    "homepage": "https://www.softleister.de",
    "license": "LGPL",
    "authors": [{
        "name": "Softleister - Hagen Klemp",
        "homepage": "https://www.softleister.de",
        "role": "Developer"
    }],
    "support": {
        "email": "info@softleister.de"
    },
    "require": {
        "php": "^7.2|^8.0",
        "contao/core-bundle": "^5.3",
        "codefog/contao-haste": "^5.2",
        "phpoffice/phpspreadsheet": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Softleister\\TimetrackerBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Softleister\\TimetrackerBundle\\ContaoManager\\Plugin"
    }
}
